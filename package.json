{
  "name": "angular-ticketmaster",
  "version": "0.0.0",
  "license": "MIT",
  "directories": {
    "output": "release",
    "app": "dist",
    "buildResources": "build"
  },
  "build": {
    "appId": "app.aw.ticketmaster"
  },
  "scripts": {
    "release": "npm run build && npm run package:all",
    "postinstall": "cpx webpack-build-common.js node_modules/angular-cli/models && cpx webpack-build-test.js node_modules/angular-cli/models",
    "ng": "ng",
    "start": "npm run dev",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "concurrently \"ng serve\" \"electron electron/entry.live.js\"",
    "prepare:build": "npm run dev && cpx 'electron/*.{json,js}' dist",
    "package:all": "npm run prepare:build && electron-packager dist Ticketmaster2 --all --out=release --overwrite",
    "package:mac": "npm run prepare:build && electron-packager dist Ticketmaster2 --platform=darwin --out=release --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0-rc.2",
    "@angular/compiler": "^4.0.0-rc.2",
    "@angular/core": "^4.0.0-rc.2",
    "@angular/forms": "^4.0.0-rc.2",
    "@angular/http": "^4.0.0-rc.2",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^4.0.0-rc.2",
    "@angular/platform-browser-dynamic": "^4.0.0-rc.2",
    "@angular/router": "^4.0.0-rc.2",
    "core-js": "^2.4.1",
    "electron": "^1.6.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.17.1",
    "ng2-facebook-sdk": "^1.2.0",
    "oauth-electron-facebook": "^0.1.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "^4.0.0-rc.2",
    "@types/jasmine": "2.5.38",
    "@types/node": "^7.0.5",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "electron-packager": "^8.5.2",
    "jasmine-core": "^2.5",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "^2.2.1"
  }
}
