{
  "name": "angular-ticketmaster",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"electron src/entry.live.js\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package:linux": "electron-packager . electron-explorer-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=linux --arch=all --overwrite",
    "package:mac": "electron-packager . electron-explorer-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=darwin --arch=all --overwrite ",
    "package:win": "electron-packager . electron-explorer-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=win32 --arch=all --overwrite ",
    "package:all": "electron-packager . electron-explorer-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --all --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0.rc.2",
    "@angular/compiler": "^4.0.0.rc.2",
    "@angular/core": "^4.0.0.rc.2",
    "@angular/forms": "^4.0.0.rc.2",
    "@angular/http": "^4.0.0.rc.2",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^4.0.0.rc.2",
    "@angular/platform-browser-dynamic": "^4.0.0.rc.2",
    "@angular/router": "^4.0.0.rc.2",
    "core-js": "^2.4.1",
    "electron": "^1.6.1",
    "hammerjs": "^2.0.8",
    "ng2-facebook-sdk": "^1.2.0",
    "oauth-electron-facebook": "^0.1.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "^4.0.0.rc.2",
    "@types/jasmine": "2.5.38",
    "@types/node": "^7.0.5",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "electron-packager": "^8.5.2",
    "jasmine-core": "^2.5",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "^2.2.1"
  }
}
